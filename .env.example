# aNEOS Environment Configuration Example
# Copy this to .env and run installation to generate unique keys

# Database
ANEOS_DATABASE_URL=sqlite:///./aneos.db

# API Configuration  
ANEOS_ENV=development
ANEOS_LOG_LEVEL=INFO
ANEOS_HOST=0.0.0.0
ANEOS_PORT=8000

# Redis (optional)
# ANEOS_REDIS_URL=redis://localhost:6379/0

# ML Configuration
ANEOS_ML_CACHE_SIZE=1000
ANEOS_FEATURE_CACHE_TTL=1800

# Monitoring
ANEOS_METRICS_INTERVAL=60
ANEOS_ALERT_COOLDOWN=300

# Security (Generated unique keys during installation)
ANEOS_SECRET_KEY=<WILL_BE_GENERATED>
ANEOS_API_KEY_SALT=<WILL_BE_GENERATED>

# API Keys (Generated unique keys during installation)
ANEOS_ADMIN_API_KEY=<WILL_BE_GENERATED>
ANEOS_ANALYST_API_KEY=<WILL_BE_GENERATED>
ANEOS_VIEWER_API_KEY=<WILL_BE_GENERATED>